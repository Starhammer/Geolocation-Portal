@model int
<div class="float-right">
    <b>@Model</b>
    <span class="ranking">
        @for (int i = 1; i <= 5; i++)
        {
            if (i <= Model)
            {
                <i class="fa fa-star"></i>
            }
            else
            {
                <i class="fa fa-star-o"></i>
            }
        }
    </span>
</div>

