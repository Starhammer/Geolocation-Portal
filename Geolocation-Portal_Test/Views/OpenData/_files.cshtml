@model IEnumerable<Geolocation_Portal_Test.Models.file>

@foreach (var item in Model)
{
    if (item.map_data == true)
    {
        <a class="nolink float-right ml-2" href="~/GeoData/index/@item.Id" title=""><i class="fa fa-map"></i></a>
    }
    else if (item.diagram_data == true)
    {
        <span><img class="float-right ml-2" width="25" height="25" src="~/icons/@Html.DisplayFor(modelItem => item.file_icon)-icon.png" /></span>
    }
    else
    {
        <span><img class="float-left mr-2" width="25" height="25" src="~/icons/@Html.DisplayFor(modelItem => item.file_icon)-icon.png" /></span>
    }

}
